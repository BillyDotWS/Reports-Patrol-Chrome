{
    "name": "Reports Patrol",
    "version": "1.0",
    "manifest_version": 3,
    "author": "Mohsin Ali",
    "homepage_url": "https://fiverr.com/mohsinaliryk",
    "background": {
        "service_worker": "sw.js"
    },
    "icons": {
        "24": "icons/24.png",
        "32": "icons/32.png",
        "48": "icons/48.png",
        "64": "icons/64.png",
        "128": "icons/128.png",
        "256": "icons/256.png"
    },
    "content_scripts": [
        {
            "matches": ["https://www.mineplex.com/threads/*"],
            "js": ["js/content.js"],
            "css": ["css/content.css"],
            "run_at": "document_end"
        }
    ],
    "host_permissions": ["*://reportspatrol.mineplex.com/*"],
    "permissions": ["cookies"]
}